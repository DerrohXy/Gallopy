{
    "name": "gallopy",
    "version": "0.0.8",
    "description": "",
    "main": "dist/index.js",
    "module": "dist/index.esm.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.esm.js",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./styles": "./dist/index.css",
        "./components": {
            "import": "./dist/components/index.esm.js",
            "require": "./dist/components/index.js",
            "types": "./dist/components/index.d.ts"
        },
        "./utils": {
            "import": "./dist/utils/index.esm.js",
            "require": "./dist/utils/index.js",
            "types": "./dist/utils/index.d.ts"
        }
    },
    "sideEffects": false,
    "scripts": {
        "rollup": "rollup --config --bundleConfigAsCjs",
        "rollup-watch": "rollup -w --config --bundleConfigAsCjs",
        "build": "tsc && npm run rollup",
        "build:watch": "tsc && npm run rollup-watch",
        "build:analyze": "tsc && npm run rollup && npx rollup-plugin-visualizer dist/stats.html",
        "size": "npm run build && npx size-limit"
    },
    "files": [
        "dist"
    ],
    "keywords": [],
    "author": "",
    "license": "MIT",
    "peerDependencies": {
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-icons": "^4.12.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^28.0.1",
        "@rollup/plugin-node-resolve": "^15.3.0",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@types/react": "^19.1.12",
        "@types/react-dom": "^19.1.9",
        "rollup": "^4.28.1",
        "rollup-plugin-dts": "^6.1.1",
        "rollup-plugin-postcss": "^4.0.2",
        "rollup-plugin-visualizer": "^5.12.0",
        "size-limit": "^11.0.2",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2"
    }
}
