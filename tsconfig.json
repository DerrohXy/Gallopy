        "lib": ["DOM", "ESNext"],
        "declaration": true,
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "nodenext",
        "skipLibCheck": true,
        "sourceMap": false,
        "jsx": "react"
    },
    "include": ["src"]
